{
  "name": "@pokedex/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "commit": "czg",
    "check-commit": "node ./scripts/commit-lint.js",
    "check-format": "nx format:check --all",
    "check-format:quick": "pretty-quick --check",
    "check-lock-files": "node ./scripts/check-lock-files.js",
    "prepare": "is-ci || husky",
    "preinstall": "node ./scripts/preinstall.js",
    "test": "nx run-many -t test",
    "e2e": "nx run-many -t e2e --projects ./e2e/*",
    "lint-package-lock": "eslint package-lock.json"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git",
      "czConfig": "./scripts/commitizen.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.3.1",
    "@nx/eslint": "21.3.1",
    "@nx/eslint-plugin": "21.3.1",
    "@nx/js": "21.3.1",
    "@nx/playwright": "21.3.1",
    "@nx/react": "21.3.1",
    "@nx/vite": "21.3.1",
    "@nx/web": "21.3.1",
    "@nx/workspace": "21.3.1",
    "@playwright/test": "^1.36.0",
    "@react-router/dev": "7.7.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "conventional-changelog-cli": "5.0.0",
    "cz-git": "1.12.0",
    "czg": "1.12.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.3.1",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.6.2",
    "pretty-quick": "4.2.2",
    "semver": "7.7.2",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "nx": {
    "includedScripts": [
      "lint-package-lock",
      "check-commit",
      "check-format",
      "check-format:quick",
      "check-lock-files"
    ]
  },
  "dependencies": {
    "@mantine/core": "8.1.3",
    "@mantine/hooks": "8.1.3",
    "@react-router/node": "7.7.0",
    "@react-router/serve": "7.7.0",
    "@tanstack/react-query": "5.83.0",
    "axios": "1.10.0",
    "axios-cache-interceptor": "1.8.0",
    "clsx": "2.1.1",
    "isbot": "^4.4.0",
    "mantine-datatable": "8.1.2",
    "pokenode-ts": "1.20.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "6.0.0",
    "react-router": "7.7.0"
  }
}
